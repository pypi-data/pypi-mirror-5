# a collection of "tricky" literals

cases = ['no quotes',
         "single ' quote",
         'double " quote',
         'triple """ quotes',
         'mixed "\'""" quotes',
         '"',
         "'",
         '"\'"',
         '\\', # len 1
         '\\"', # len 2
         '\\\\"', # len 3
         '\\"\\', # len 3
         '<a some="typical" html="content">here</a>',
         ]
