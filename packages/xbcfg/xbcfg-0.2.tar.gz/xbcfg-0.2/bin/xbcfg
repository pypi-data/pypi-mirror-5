#!/usr/bin/env python

import sys
from xbcfg.xbcfg import main

args = sys.argv[1:]
try:
    if not args:
        raise KeyError()
    main(*args)
except KeyError:
    print "usage: %s reset|bootloader|download|flash|reset [extra arguments]" % sys.argv[0]
    print
    print "XBee configuration and flashing utility"
    exit(255)

