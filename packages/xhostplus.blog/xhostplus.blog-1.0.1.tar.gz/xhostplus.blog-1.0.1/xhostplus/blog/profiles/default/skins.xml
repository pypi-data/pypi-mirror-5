<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False">

 <object name="xhostplus_blog_templates"
    meta_type="Filesystem Directory View"
    directory="xhostplus.blog:skins/xhostplus_blog_templates"/>
 <object name="xhostplus_blog_images"
    meta_type="Filesystem Directory View"
    directory="xhostplus.blog:skins/xhostplus_blog_images"/>
 <object name="xhostplus_blog_javascripts"
    meta_type="Filesystem Directory View"
    directory="xhostplus.blog:skins/xhostplus_blog_javascripts"/>
 <object name="xhostplus_blog_styles"
    meta_type="Filesystem Directory View"
    directory="xhostplus.blog:skins/xhostplus_blog_styles"/>

 <skin-path name="*">
  <layer name="xhostplus_blog_templates"
     insert-after="custom"/>
  <layer name="xhostplus_blog_images"
     insert-after="xhostplus_blog_templates"/>
  <layer name="xhostplus_blog_javascripts"
     insert-after="xhostplus_blog_images"/>
  <layer name="xhostplus_blog_styles"
     insert-after="xhostplus_blog_javascripts"/>
 </skin-path>

</object>
