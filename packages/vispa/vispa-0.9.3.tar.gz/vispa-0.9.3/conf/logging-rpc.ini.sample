[loggers]
keys=root,RPCmain

[handlers]
keys=net

[formatters]
keys=simpleFormatter

[logger_root]
level=ERROR
handlers=

[logger_RPCmain]
level=INFO
handlers=
qualname=RPCmain

[handler_net]
class=logging.handlers.SocketHandler
level=DEBUG
formatter=simpleFormatter
args=("localhost",50000)

[formatter_simpleFormatter]
format=%(asctime)s - %(name)s - %(levelname)s - %(message)s
datefmt=
