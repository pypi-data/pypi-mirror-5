<metal:block metal:define-macro="main"
             tal:define="totime python:context.getAdapter('totime')"
             i18n:domain="mediasearch">
    <table class="table table-bordered">
        <tbody>
            <tr>
                <td i18n:translate="">
                    Id
                </td>
                <td>
                    <tal:block tal:content="here/getId"/>
                </td>
            </tr>

            <tr>
                <td i18n:translate="">
                    Name
                </td>
                <td>
                    <tal:block tal:content="here/Title"/>
                </td>
            </tr>
            <tr>
                <td i18n:translate="">
                    Size
                </td>
                <td>
                    <tal:block tal:content="here/getObjSize"/>
                </td>
            </tr>
            <tr>
                <td i18n:translate="">
                    Mime type
                </td>
                <td>
                    <tal:block tal:content="here/getContentType"/>
                </td>
            </tr>
            <tr>
                <td i18n:translate="">
                    UID
                </td>
                <td>
                    <tal:block tal:content="here/UID"/>
                </td>
            </tr>
            <tr>
                <td i18n:translate="">
                    Height
                </td>
                <td>
                    <tal:block tal:content="here/getHeight"/>
                </td>
            </tr>
             <tr>
                <td i18n:translate="">
                    Width
                </td>
                <td>
                    <tal:block tal:content="here/getWidth"/>
                </td>
            </tr>
             <tr>
                <td i18n:translate="">
                    Created
                </td>
                <td>
                    <tal:block tal:content="python:totime(here.created())"/>
                </td>
            </tr>
             <tr>
                <td i18n:translate="">
                    Modified
                </td>
                <td>
                    <tal:block tal:content="python:totime(here.modified())"/>
                </td>
            </tr>
        </tbody>
    </table>
</metal:block>