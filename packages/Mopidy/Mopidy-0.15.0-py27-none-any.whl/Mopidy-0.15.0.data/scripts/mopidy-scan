#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'Mopidy==0.15.0','console_scripts','mopidy-scan'
__requires__ = 'Mopidy==0.15.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('Mopidy==0.15.0', 'console_scripts', 'mopidy-scan')()
    )
