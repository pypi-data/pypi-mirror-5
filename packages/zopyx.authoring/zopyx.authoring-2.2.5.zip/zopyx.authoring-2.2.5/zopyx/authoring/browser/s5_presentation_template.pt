<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title tal:content="options/title" />

	<!-- metadata -->

	<!-- "generator" can be the program used to generate this, or (by default) an indication that it's an S5 slide show. -->
	<meta name="generator" content="S5" />
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />

	<!-- "version" identifies the particular version of S5 -->
	<meta name="version" content="S5 1.1" />

	<!-- "presdate" identifies the date of the presentation.  Example: "20061231" is December 31 2006 -->
	<meta name="presdate" content="20061231" />

	<meta name="author" content="[name of author]" />
	<meta name="company" content="[name of company/organization]" />

	<!-- configuration parameters -->
		<!-- Set defaultView to "outline" if you want the default view to be as a plain web page; otherwise, use "slideshow." -->
		<meta name="defaultView" content="slideshow" />

		<!-- Set controlVis to "hidden" if you want the controls hidden.  Otherwise, use "visible."  The default is "visible." -->
		<meta name="controlVis" content="hidden" />

	<!-- style sheet links -->
	<!-- these control the visual appearance of the slide show. -->
	<link rel="stylesheet" href="pp-s5-slides.css" type="text/css" media="projection" id="slideProj" />
	<link rel="stylesheet" href="pp-s5-outline.css" type="text/css" media="screen" id="outlineStyle" />
	<link rel="stylesheet" href="pp-s5-print.css" type="text/css" media="print" id="slidePrint" />
	<link rel="stylesheet" href="pp-s5-opera.css" type="text/css" media="projection" id="operaFix" />
	<link rel="stylesheet" href="pp-s5-pretty.css" type="text/css" media="projection" id="operaFix" />
	<link rel="stylesheet" href="pp-s5-custom.css" type="text/css" media="screen" id="operaFix" />

	<!-- S5 JS -->
	<!-- this script handles all the interactivity, e.g. making the slides advance -->
	<script src="pp-s5-slides.js" type="text/javascript"></script>
</head>
<body>

	<div class="layout">
		<div id="controls"><!-- DO NOT EDIT --></div>
		<div id="currentSlide"><!-- DO NOT EDIT --></div>

		<div id="header">
			<div id="logo"><!-- DO NOT EDIT --></div>
		</div>
	</div>

	<div id="footer">
	</div>

    <div class="presentation">
        <div class="slide">
            <div class="s5line1">
                <span tal:replace="options/s5line1|nothing" />
                "<span tal:replace="structure options/title|nothing" />"
            </div>
            <div class="s5line2">
                <span tal:replace="options/s5line2|nothing" />
            </div>
            <div class="title-logo"><img src="titlelogo.png" /></div>
            <div class="s5line3">
                <span tal:replace="options/s5line3|nothing" />
            </div>
            <div class="s5line3">
                <span tal:replace="options/s5line4|nothing" />
            </div>
        </div>

        <div class="slide" tal:repeat="slide options/slides" tal:content="structure slide" />
    </div>

</body>
</html>
