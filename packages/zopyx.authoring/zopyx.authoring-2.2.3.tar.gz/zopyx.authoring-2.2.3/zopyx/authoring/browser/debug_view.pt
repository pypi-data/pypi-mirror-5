<html xmlns="http://www.w3.org/1999/xhtml"
      xml:lang="en"
      lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plone">


    <metal:fill fill-slot="main">

        <dl class="enableFormTabbing" tal:define="result view/debugInfo">
           <dt id="fieldsetlegend-links" class="switcher">Links</dt>
           <dd id="fieldset-links">
               <div tal:repeat="d result">
                   <h3>
                        <a tal:attributes="href d/url" tal:content="string: ${d/title} (${d/path})" />
                   </h3>
                   <ul tal:repeat="d result">
                       <li tal:repeat="link d/links">
                           <a tal:attributes="href link"
                              tal:content="link" 
                           />
                       </li>
                   </ul>
                </div>
           </dd>
           <dt id="fieldsetlegend-anchors" class="switcher">Anchors</dt>
           <dd id="fieldset-anchors">
               <div tal:repeat="d result">
                   <h3>
                        <a tal:attributes="href d/url" tal:content="string: ${d/title} (${d/path})" />
                   </h3>
                   <ul tal:repeat="d result">
                       <li tal:repeat="anchor d/anchors">
                            <span tal:replace="string:#$anchor"/>
                       </li>
                   </ul>
                </div>
           </dd>
           <dt id="fieldsetlegend-images" class="switcher">Images</dt>
           <dd id="fieldset-images">
               <div tal:repeat="d result">
                   <h3>
                        <a tal:attributes="href d/url" tal:content="string: ${d/title} (${d/path})" />
                   </h3>
                   <ul tal:repeat="d result">
                       <li tal:repeat="image d/images">
                           <img tal:condition="nothing" tal:attributes="src image"/>
                           <span tal:content="image" />
                       </li>
                   </ul>
                </div>
           </dd>
        </dl>
    </metal:fill>
</html>
