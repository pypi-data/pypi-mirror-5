<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      xml:lang="en" lang="en"
      metal:use-macro="context/prefs_main_template/macros/master"
      i18n:domain="plone">

<body>

<metal:main fill-slot="prefs_configlet_content">

  <div class="documentEditable documentContent">

      <div metal:use-macro="context/global_statusmessage/macros/portal_message">
        Portal status message
      </div>

      <div id="content">
        <metal:block use-macro="view/base_template/macros/header" />
        <tal:block i18n:domain="xhostplus.social">
            <h1 class="documentFirstHeading"
                i18n:translate="">Social Network Configuration</h1>
        </tal:block>

        <div id="content-core">
            <a href=""
               class="link-parent"
               tal:attributes="href string:$portal_url/plone_control_panel"
               i18n:translate="label_up_to_plone_setup">
              Up to Site Setup
            </a>

            <metal:block use-macro="view/base_template/macros/master" />
        </div>
      </div>
  </div>
</metal:main>
</body>
</html>
