#globalnavCDR {
    /* Navigational Plone Tabs(tm), implemented by customizing the a tag - they are surprisingly elegant. The power of CSS runs strong in these :) */
    background-color: transparent;
    border-collapse: collapse;
    border-bottom-color: #577EB3;
    border-bottom-style: solid;
    border-bottom-width: 1px;
    padding: 0em 0em 0em 1.5em;
    white-space: nowrap;
    list-style: none;
    margin: 0;
    height: auto;
    line-height: normal;
    text-align:left;
    font-size:85%;
}

#globalnavCDR li {
    display: inline;
}
#globalnavCDR li a {
    /* The normal, unselected tabs. They are all links */
    background-color: transparent;
    border-color: #577EB3;
    border-left: 1px;
    border-right: 2px;
    border-top: 1px;
    border-bottom: 1px;     
    border-style: solid solid none solid;
    color: #577EB3;
    margin-right: 0.5em;
    padding: 0em 0.5em;
    text-decoration: none;
}
#globalnavCDR li.selected a {
    /* The selected tab. There's only one of this */
    background-color: #DEE7EC;
    border-color: #577EB3;
    border-left: 1px;
    border-right: 2px;
    border-top: 1px;    
    border-style: solid solid none solid;
    border-bottom: #DEE7EC 1px solid;
    color: #577EB3;
    font-weight:bold;
}
#globalnavCDR li a:hover {
    background-color: #99cccc;
    border-color: #577EB3;
    border-bottom-color: #577EB3;
    color:#577EB3;
}
#subglobalnavCDR {
    /* Navigational Plone Tabs(tm), implemented by customizing the a tag - they are surprisingly elegant. The power of CSS runs strong in these :) */
    background-color: #DEE7EC;
    border-collapse: collapse;
    border-left: #577EB3 1px solid;
    border-right: #577EB3 2px solid;
    padding: 0.1em 0em 0.3em 0.5em;
    white-space: nowrap;
    list-style: none;
    margin: 0em 0em -3em 0em;
    height: auto;
    line-height: normal;
}

#subglobalnavCDR li {
    display: inline;
    background-color: transparent;
    border-color: #f4823f;
    border-width: 0px; 
    border-style: none none none solid;
    color: #577EB3;
    height: auto;
    margin-right: 0.5em;
    text-decoration: none;
    font-size: 85%;
}
#subglobalnavCDR li a {
    /* The normal, unselected tabs. They are all links */
    background-color: transparent;
    border-color: #99cccc;
    border-width: 0px; 
    border-style: none none none solid;
    color: #577EB3;
    height: auto;
    margin-right: 0.5em;
    text-decoration: none;
    font-size: 85%;
}
#subglobalnavCDR li.selected a {
    /* The selected tab. There's only one of this */
    background-color: #99cccc;
    border: 0px solid #99cccc;
    border-bottom: #dee7ec 0px solid;
    color: #577EB3;
    font-weight: bold;
    font-style: italic;	
}
#subglobalnavCDR li a:hover {
    background-color: #99cccc;
    border-color: #99cccc;
    border-bottom-color: #99cccc;
    color: #577EB3;
    font-weight: bold;
    font-style: italic;
}
#subglobalnavCDR li.print a {
    
    text-align: right;
}
