div.box {
   border: none;
   margin: 0em 0em 0em 0em;
   padding: 0;
   white-space: nowrap;
}
div.box h4 {
   font-size: 1em;
}
div.box h5 {
   background: #fdeab9;
   border: #f4823f 1px;   
   border-style: solid solid none solid;
   color: #f4823f;
   padding: 0em 1em 0em 1em;
   display: inline;
   font-size: 1em;
   height: 1em;
}
div.box h6 {
   background: #fde893;
   border: #f4823f;
   border-style: solid solid none solid;
   color: #f4823f;
   padding: 0em 1em 0em 1em;
   display: block;font-size: 1em;
   height: 1.2em;
}
div.box div.body {
   background: transparent;
   border-collapse: collapse;
   border: solid 1px #577EB3;
}
div.box .content {
   padding: 1em;
   height: 100%;
}
div.box a.close {
   float: right;
   text-transform: none;
   border-left: #f4823f;
   padding: 0em 0.2em;
}
div.box h1,
div.box h2,
div.box h3,
div.box h4 {
   margin: 0;
   padding: 0;
}
div.box .even {
   background-color: #fde893;
}
div.box .odd {
   background-color: #fdeab9;
}
div.box input {
   font-size: 100%;
}
div.spacer {
   margin: 1em;
}
table.columns {
   width: 100%;
}
table.columns td.left {
   vertical-align: top;
   width: 15%;
   padding: 2em 0em 1em 2em;
}
table.columns td.main {
   vertical-align: center;
   padding: 0em 2em 1em 2em;
   margin: 0;
}
table.columns td.right {
   vertical-align: top;
   width: 15%;
   padding: 2em 2em 1em 0em;
}
