<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="${default_skin}">
    <object name="${namespace_package}_${package}"
        meta_type="Filesystem Directory View"
        directory="${package_dotted_name}:skins/${namespace_package}_${package}"/>
    <skin-path name="*">
        <layer name="${namespace_package}_${package}"
                insert-after="custom"/>
    </skin-path>
</object>
