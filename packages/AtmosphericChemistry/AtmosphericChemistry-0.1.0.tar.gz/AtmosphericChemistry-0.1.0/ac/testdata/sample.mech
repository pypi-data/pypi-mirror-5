FUNCTIONS
# Troe expression for 3-body reactions
k0(k0_300, n) = k0_300 * (300./T)^n
kinf(kinf_300, m) = kinf_300 * (300./T)^m
kratio() = k0() * M / kinf()
kfexp() = 1./(1. + log(kratio())^2)
ktroe(k0_300, n, kinf_300, m, fc) = k0()*M/(1.+kratio()) * fc^kfexp() 

# activation reaction (CO + OH + M) -- uses the same kratio and kfexp as ktroe
kactivation(k0_300, n, kinf_300, m, fc) = k0()/(1.+kratio()) * fc^kfexp()

N(fc) = 0.75 - 1.27 * log(fc)
kfexp_iupac() = 1./(1. + log(kratio()/N())^2)
k_3rd_iupac(k0_300, n, kinf_300, m, fc) = k0()*M/kratio() * fc^kfexp_iupac() 

END

