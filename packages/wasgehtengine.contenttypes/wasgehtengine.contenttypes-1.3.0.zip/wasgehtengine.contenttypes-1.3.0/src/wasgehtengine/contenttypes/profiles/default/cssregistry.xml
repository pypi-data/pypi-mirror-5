<?xml version="1.0"?>
<object name="portal_css">

	<stylesheet
		id="++resource++wasgehtengine.contenttypes/css/3rdparty/leaflet/leaflet.css"
		expression="python:context.getPortalTypeName() == 'wasgehtengine.Venue' or context.getPortalTypeName() == 'wasgehtengine.Event'"
		cacheable="False" cookable="False" compression="none" media="all" />
	
	<stylesheet id="++resource++wasgehtengine.contenttypes/css/map.css"
		expression="python:context.getPortalTypeName() == 'wasgehtengine.Venue' or context.getPortalTypeName() == 'wasgehtengine.Event' or context.REQUEST['ACTUAL_URL'].endswith('++add++wasgehtengine.Venue')"
		cacheable="False" cookable="False" compression="safe" media="all" />
</object>
