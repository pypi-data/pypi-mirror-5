<?xml version="1.0"?>
<object name="portal_properties" 
	meta_type="Plone Properties Tool" 
	xmlns:i18n="http://xml.zope.org/namespaces/i18n" 
	i18n:domain="plone">
  <object name="navtree_properties" meta_type="Plone Property Sheet">
    <property name="metaTypesNotToList" type="lines">
      <element value="School"/>
      <element value="Division"/>
      <element value="Course"/>
    </property>
  </object>
  <object name="site_properties" meta_type="Plone Property Sheet">
    <property name="title">Site wide properties</property>
    <property name="allowAnonymousViewAbout" type="boolean">False</property>
    <property name="localTimeFormat" type="string">%m-%d-%Y</property>
    <property name="localLongTimeFormat"
	      type="string">%m-%d-%Y %I:%M %p %Z</property>
    <property name="allowRolesToAddKeywords" type="lines">
      <element value="Producer"/>
      <element value="Administrator"/>
      <element value="Manager"/>
    </property>
    <property name="default_page" type="lines"/> 
    <property name="forbidden_contenttypes" type="lines">
      <element value="text/x-python"/>
      <element value="text/x-web-intelligent"/>
    </property>
    <property name="enable_livesearch" type="boolean">False</property>
  </object>
  <object name="educommons_properties" meta_type="Plone Property Sheet">
    <property name="title">eduCommons properties</property>
    <property name="school_enable" type="boolean">False</property>
    <property name="school_descriptor" type="string" i18n:translate="">Schools</property>
    <property name="division_descriptor" type="string" i18n:translate="">Divisions</property>
    <property name="course_descriptor" type="string" i18n:domain="eduCommons" i18n:translate="">Course Contents</property>
    <property name="reusecourse_instance" type="string">http://openocw.org</property>
    <property name="workflow_order" type="lines">
      <element value="InProgress" i18n:attributes="value" />
      <element value="QA" i18n:attributes="value" />
      <element value="Released" i18n:attributes="value" />
     <element value="Published" i18n:attributes="value" />
    </property>
    <property name="oerrecommender_enabled" type="boolean">False</property>
    <property name="reusecourse_enabled" type="boolean">True</property>
  </object>
  <object name="content_licensing_properties" meta_type="Plone Property Sheet">
    <property name="DefaultSiteLicense" type="lines">
      <element value="Creative Commons License" />
      <element value="Attribution-Noncommercial-Share Alike 3.0" />
      <element value="http://creativecommons.org/licenses/by-nc-sa/3.0/" />
      <element value="http://i.creativecommons.org/l/by-nc-sa/3.0/88x31.png" />
    </property>
  </object>
