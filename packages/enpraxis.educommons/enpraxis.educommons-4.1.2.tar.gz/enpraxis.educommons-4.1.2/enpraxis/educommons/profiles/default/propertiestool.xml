  <object name="oaiintercom_properties"
	  meta_type="Plone Property Sheet">
    <property name="provideMetadataFor" type="lines">
      <element value="Schools"/>
      <element value="Division"/>
      <element value="Course"/>
    </property> 	
  </object>
  <object name="bookmarklets_properties" meta_type="Plone Property Sheet">
