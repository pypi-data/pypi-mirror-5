<div id="about" i18n:domain="eduCommons">
  <table id="aboutTable"
	 border="0" 
	 cellspacing="0"
	 cellpadding="0" 
	 i18n:attributes="summary"
	 summary="Information about Professor">
    <tbody>
      <tr>
	<td id="aboutPhoto">
	  <!-- Use eduCommons insert link functionality to create this link -->        
	  <img width="275" height="275" border="0"
	       title="Professor Image"
	       alt="Professor Image" 
               src=""
	       i18n:attributes="title;
				alt"   
               tal:attributes="src string:eduCommonsDivision.gif"/>
	</td>
	<td id="aboutInfo">
	  <h3 i18n:translate="">Professor X, Ph.D.</h3>
	  <p i18n:translate="">Department Head / Professor</p>
	  <p i18n:translate="">Department Name<br /> 
	    Institution Name</p>
	  <p i18n:translate="">Additional notes about Professor X.</p>
	</td>
      </tr>
    </tbody>
  </table>
</div>
