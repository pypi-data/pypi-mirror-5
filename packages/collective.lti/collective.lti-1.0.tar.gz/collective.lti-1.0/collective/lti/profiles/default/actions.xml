<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
	xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <object name="document_actions" meta_type="CMF Action">
    <object name="lti" meta_type="CMF Action" i18n:domain="collective.lti">
      <property name="title" i18n:translate="">LTI Export</property>
      <property name="description" i18n:translate="">Create LTI launch point here</property>
      <property name="url_expr">string:$object_url/ltimanage</property>
      <property name="link_target"/>
      <property name="icon_expr">string:$portal_url/++resource++collective.lti/ltiexport.png</property>
      <property name="available_expr">python:context.restrictedTraverse('@@plone_interface_info').provides('collective.lti.interfaces.ILTITransformable')</property>
      <property name="permissions"/>
      <property name="visible">True</property>
    </object>
  </object>
</object>
