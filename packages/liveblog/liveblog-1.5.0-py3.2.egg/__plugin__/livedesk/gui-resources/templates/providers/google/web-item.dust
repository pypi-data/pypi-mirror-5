{#results}<li class="google draggable" idx="{@idx}{.}{/idx}" startx="{startx}">	<div class="drag-bar"></div>	<div class="result-content">		<h3><a href="{url}" target="_blank">{title|s}</a></h3>		<p class="result-text">{content|s}</p>		<p class="attributes">			<i class="source-icon">				<img src="http://g.etfv.co/{url}" style="max-width: 16px" border="0">			</i>			<a class="author-name" href="{url}" target="_blank">{visibleUrl}</a>			<time>{date}</time>		</p>    </div></li>{/results}