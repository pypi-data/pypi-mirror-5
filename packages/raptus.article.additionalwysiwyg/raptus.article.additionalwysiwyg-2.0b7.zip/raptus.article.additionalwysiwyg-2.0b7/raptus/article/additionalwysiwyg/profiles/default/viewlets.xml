<?xml version="1.0"?>
<object>

 <order manager="plone.abovecontenttitle" skinname="*">
  <viewlet name="raptus.article.wysiwyg.left" insert-before="*" />
  <viewlet name="raptus.article.wysiwyg.right" insert-before="*" />
 </order>

 <order manager="plone.abovecontentbody" skinname="*">
  <viewlet name="raptus.article.wysiwyg.full" insert-before="*" />
  <viewlet name="raptus.article.wysiwyg.top" insert-before="*" />
 </order>

 <order manager="plone.belowcontent" skinname="*">
  <viewlet name="raptus.article.wysiwyg.bottom" insert-after="*" />
 </order>

 <hidden manager="plone.abovecontentbody" skinname="*">
  <viewlet name="raptus.article.wysiwyg.top" />
 </hidden>

 <hidden manager="plone.belowcontent" skinname="*">
  <viewlet name="raptus.article.wysiwyg.bottom" />
 </hidden>

</object>
