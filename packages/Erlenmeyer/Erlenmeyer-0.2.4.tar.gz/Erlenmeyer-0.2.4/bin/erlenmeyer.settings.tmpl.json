{
    "metadata":
    {
        "fileName": "{{ metadata.fileName }}",
        "projectName": "{{ metadata.projectName }}",
        "projectOwner": "{{ metadata.projectOwner }}",
        "fileAuthor": "{{ metadata.fileAuthor }}",
        "pubDate": "{{ metadata.pubDate }}"
    },

    "server":
    {
        "ip": "0.0.0.0",
        "port": 8080,
