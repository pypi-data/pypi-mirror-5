#!/usr/bin/env mio

TGreeting = Object clone do (
    hello = method(
        writeln("Hello", self getGreeting)
    )
)

World = Object clone do (
    uses(TGreeting)

    greeting = "World!"

    getGreeting = method(
       return (self greeting)
    )

    setGreeting = method(aGreeting,
        self greeting = aGreeting
    )
)
