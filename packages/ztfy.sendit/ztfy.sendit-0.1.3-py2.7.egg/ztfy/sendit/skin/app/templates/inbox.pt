<div class="span12" i18n:domain="ztfy.sendit">
	<div class="widget-box">
		<div class="widget-title">
			<span class="icon">
				<i class="icon-inbox"></i>
			</span>
			<h5 i18n:translate="">List of packets intended to you</h5>
		</div>
		<div class="widget-content nopadding"
			 tal:condition="view/values"
			 tal:content="structure view/renderTable" />
		<div class="widget-content"
			 tal:condition="not:view/values"
			 i18n:translate="">You don't have any packet in your inbox.</div>
	</div>
</div>
