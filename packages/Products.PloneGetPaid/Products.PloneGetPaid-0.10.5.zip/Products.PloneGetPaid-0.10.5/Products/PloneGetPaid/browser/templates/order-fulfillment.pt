<fieldset i18n:domain="plonegetpaid">

  <legend i18n:translate="legend_order_fulfillment"> Fulfillment </legend>

  <span i18n:translate="text_current_status"> Current Status </span>  - <span tal:content="view/fulfillment_status" i18n:translate=""> Processing </span>  
  
  <form action="."
        tal:attributes="action request/URL" method="post"
        class="edit-form" enctype="multipart/form-data"
        id="getpaid.cart.actions">

    <div class="form-controls" tal:condition="view/availableActions">
      <input tal:repeat="action view/actions"
             tal:replace="structure action/render"
             />
    </div>
  </form>
    
</fieldset>

