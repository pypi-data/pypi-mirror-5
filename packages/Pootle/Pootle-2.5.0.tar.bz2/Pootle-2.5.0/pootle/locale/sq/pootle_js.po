msgid "<a href=\"%s\">String %s</a>"
msgstr "<a href=%s> Parametër %s</a>"

#: static/js/editor.js:786
msgid "Error while connecting to the server"
msgstr "Gabim kur lidhemi ne serverin"

#: static/js/editor.js:788
msgid "Server error"
msgstr "Gabim i serverit"

#: static/js/editor.js:790
msgid "The server seems down. Try again later."
msgstr "Ky server duket i fikur. Provo me vone."

#: static/js/editor.js:798
msgid "Unknown error"
msgstr "Gabim i panjohur"

#: static/js/editor.js:1342
msgid "Congratulations, you walked through all strings."
msgstr "Pergezime, ju kaluat te gjithe stringat."

#: static/js/editor.js:1344 static/js/editor.js.c:1368
msgid "Return to the overview page."
msgstr "Ktheu prapa te rishikoni faqen."

#: static/js/editor.js:1363
msgid "You reached the beginning of the list"
msgstr "Ju aritet ne fillimin e listes"

#: static/js/editor.js:1366
msgid "You reached the end of the list."
msgstr "Ju aritet ne fund te listes."

#. Translators: This is the quality match percentage of a TM result.
#. '%s' will be replaced by a number, and you should keep the extra
#. '%' symbol to denote a percentage is being used.
#: static/js/editor.js:1766
#, c-format
msgid "%s% match"
msgstr "%s% përputhet"

