/* <dtml-with base_properties> */
/* <dtml-call "REQUEST.set('portal_url', portal_url())"> (not this either :) */

.media .wrapped {
  overflow:hidden;
  padding-left:0;
  padding-right:0;
}

.media .wrapped > img {
  display:block;
  float:right;
  margin-left:2em;
}

.media h2 {
  margin:1.5em 0 0 0;
  text-transform:uppercase;
  font-size:1.333333333333333em;
  line-height:1.25em;
}

.media h2 small {
  display:block;
  font-size:1.4em;
  line-height:1.25em;
}

.media li {
  margin:0;
}

.media ul.manageableList li .wrapped {
  padding-top:30px;
}

.media ul.manageableList ul {
  white-space:nowrap;
  overflow:auto;
  height:550px;
  margin:0;
  padding:30px 0;
}

.media ul.manageableList ul li {
  vertical-align:bottom;
  display:inline-block;
  margin:0 15px;
  padding-top:0;
}

.media ul.manageableList ul li img {
  display:block;
  max-height:100%;
}

.media ul.manageableList ul li object,
.media ul.manageableList ul li .autoFlowPlayer {
  width:780px;
  height:100%;
  max-height:500px;
}

.media-enhanced {
  position:relative;
  height:680px;
  overflow:hidden;
}

.media-enhanced ul.manageableList {
  position:relative;
}

.media-enhanced ul.manageableList ul {
  overflow:hidden;
}

.media-enhanced .nav-wrapper {
  position:relative;
  height:0;
  overflow:visible;
}

.media-enhanced .nav-wrapper ul.nav {
  position:absolute;
  top:30px;
  right:0;
  z-index:1;
  padding:0 40px;
  text-align:right;
}

.media-enhanced ul.nav li {
  display:inline-block;
  margin:0 0 4px 4px;
}

.media-enhanced ul.nav li img,
.media-enhanced ul.nav li a {
  display:inline-block;
}

.media-enhanced ul.manageableList li .wrapped {
  position:relative;
}

.media-enhanced ul.manageableList li .wrapped .manage {
  right:auto;
  left:0;
}

.media-enhanced ul.manageableList ul {
  height:auto;
}

.media-enhanced ul.manageableList ul li {
  float:left;
  display:block;
  position:relative;
  height:auto;
}

.media-enhanced ul.manageableList ul li .next,
.media-enhanced ul.manageableList ul li .prev {
  display:none;
  position:absolute;
  color:#fff;
  top:50%;
  left:-90px;
  z-index:1;
  margin-top:-35px;
  width:60px;
  height:70px;
  overflow:hidden;
  text-indent:-9999px;
  background-color:#333;
}

.media-enhanced ul.manageableList ul li .next:after,
.media-enhanced ul.manageableList ul li .prev:after,
.media-enhanced ul.manageableList ul li .next:before,
.media-enhanced ul.manageableList ul li .prev:before {
  content:" ";
  display:block;
  position:absolute;
  left:50%;
  top:50%;
  margin:-16px 0 0 -8px;
  width:0;
  height:0;
  border:16px solid transparent;
  border-left-color:#fff;
}

.media-enhanced ul.manageableList ul li .next:after {
  margin-left:-9px;
  border-left-color:#333;
}

.media-enhanced ul.manageableList ul li .prev:before {
  border-left-color:transparent;
  border-right-color:#fff;
  margin-left:-24px;
}

.media-enhanced ul.manageableList ul li .prev:after {
  margin-left:-23px;
  border-right-color:#333;
  border-left-color:transparent;
}

.media-enhanced ul.manageableList ul li .next:hover,
.media-enhanced ul.manageableList ul li .prev:hover {
  background-color:#222;
}

.media-enhanced ul.manageableList ul li .next:hover:after {
  border-left-color:#222;
}

.media-enhanced ul.manageableList ul li .prev:hover:after {
  border-right-color:#222;
}

.media-enhanced ul.manageableList ul li .next {
  left:auto;
  right:-90px;
}

.media-enhanced ul.manageableList ul li.current .next,
.media-enhanced ul.manageableList ul li.current .prev {
  display:block;
}

.media-enhanced ul.manageableList ul li.arrows-inside .next {
  right:0;
}

.media-enhanced ul.manageableList ul li.arrows-inside .prev {
  left:0;
}

.media-enhanced ul.nav li a,
.media-enhanced ul.manageableList ul li {
  position:relative;
  -ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=40)";
  filter:alpha(opacity=40);
  -moz-opacity:0.4;
  -khtml-opacity:0.4;
  opacity:0.4;
  transition:all 0.5s linear;
  -webkit-transition:all 0.5s linear;
  -moz-transition:all 0.5s linear;
  -o-transition:all 0.5s linear;
}

.media-enhanced ul.manageableList ul li.ready object,
.media-enhanced ul.manageableList ul li.ready embed {
  width:100%;
  height:100%;
}

.media-enhanced ul.nav li img,
.media-enhanced ul.manageableList ul li > img {
  cursor:pointer;
  transition:all 0.5s linear;
  -webkit-transition:all 0.5s linear;
  -moz-transition:all 0.5s linear;
  -o-transition:all 0.5s linear;
  filter:grayscale(100%);
  -webkit-filter:grayscale(100%);
  -moz-filter:grayscale(100%);
  -ms-filter:grayscale(100%);
  -o-filter:grayscale(100%);
  filter:url("data:image/svg+xml;base64,PHN2ZyB4bWxucz0iaHR0cDovL3d3dy53My5vcmcvMjAwMC9zdmciPg0KIDxmaWx0ZXIgaWQ9ImdyYXlzY2FsZSI+DQogIDxmZUNvbG9yTWF0cml4IHR5cGU9Im1hdHJpeCIgdmFsdWVzPSIwLjMzMzMgMC4zMzMzIDAuMzMzMyAwIDAgMC4zMzMzIDAuMzMzMyAwLjMzMzMgMCAwIDAuMzMzMyAwLjMzMzMgMC4zMzMzIDAgMCAwIDAgMCAxIDAiLz4NCiA8L2ZpbHRlcj4NCjwvc3ZnPg==#grayscale");
  filter:gray;
  -webkit-filter:grayscale(1);
}

.media-enhanced ul.nav li a:hover,
.media-enhanced ul.nav li.current a,
.media-enhanced ul.manageableList ul li.current {
  -ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
  filter:alpha(opacity=100);
  -moz-opacity:1;
  -khtml-opacity:1;
  opacity:1;
}

.media-enhanced ul.nav li a:hover img,
.media-enhanced ul.nav li.current img,
.media-enhanced ul.manageableList ul li.current > img {
  filter:none;
  -webkit-filter:grayscale(0);
}

.media-enhanced ul.nav li.current a:after {
  content:" ";
  display:block;
  position:absolute;
  left:0;
  top:0;
  width:100%;
  height:100%;
  background-color:rgba(246, 214, 3, 0.6);
}

.media-enhanced ul.manageableList ul li.no-animation,
.media-enhanced ul.manageableList ul li.no-animation > img {
  transition:none;
  -webkit-transition:none;
  -moz-transition:none;
  -o-transition:none;
}

/* </dtml-with> */