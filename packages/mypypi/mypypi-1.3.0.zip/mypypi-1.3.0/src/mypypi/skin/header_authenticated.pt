<a href="/" tal:attributes="href context/@@siteURL">
  <div id="logo">&nbsp;</div>
</a>
<div id="duck">&nbsp;</div>
<div id="jclock"></div>
<div id="search" tal:condition="context/@@canView">
  <form action="/search.html" method="post" enctype="multipart/form-data"
        name="searchForm" id="searchForm"
        tal:attributes="action view/searchURL">
    <div id="searchButtons">
      <div class="label">
        <label for="search-widgets-searchText">
          <span>Search </span>
        </label>
      </div>
      <div class="widget">
      <input id="search-widgets-searchText"
             name="search.widgets.searchText"
             class="text-widget textline-field" value=""
             type="text" />
      <input id="search-buttons-search"
             name="search.buttons.search"
             class="submit-widget button-field" value="Search"
             type="submit" />
    </div>
  </form>
</div>
<div id="user">
  <span i18n:translate="">User:</span>
  <span tal:content="request/principal/title">Manager</span>
</div>
