<configure xmlns="http://namespaces.zope.org/zope"
           xmlns:browser="http://namespaces.zope.org/browser">

  <browser:page
    for="*"
    name="exampleformview"
    template="exampleformview.pt"
    permission="zope2.View"
  />
  
  <browser:page
    for="*"
    name="exampleform"
    class=".exampleform.ExampleFormRenderer"
    permission="zope2.View"
  />

</configure>