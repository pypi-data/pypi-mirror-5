<script type="text/javascript" tal:content="view/get_variables"></script>
<h5 class="hiddenStructure">Controlli video</h5>
<ul id="video-navigation">
  <li class='jwplayerplay' style="display:inline"><a href="#" onclick="jwplayer().play();return false;">Play</a></li>
  <li class='jwplayerpausa' style="display:inline"><a href="#" onclick="jwplayer().pause();return false;">Pausa</a></li>
  <li class='jwplayerstop' style="display:inline"><a href="#" onclick="jwplayer().stop();return false;">Stop</a></li>
  <li class='jwplayeraudio' style="display:inline"><a href="#" onclick="jwplayer().setMute();return false;">Audio</a></li>
</ul>
<div id="container"><p id="jwplayer_message">Loading the player ...</p></div>
<script type="text/javascript">
  if (typeof jwplayer_file === 'undefined') {
    jq('div p#jwplayer_message').text('Impossibile caricare il video');
  }
  else {
    jwplayer("container").setup({
      flashplayer: "++resource++collective.rtvideo.mediacore.jwplayer/player.swf",
      height: jwplayer_h,
      width: jwplayer_w,
      provider: 'http',
      controlbar: 'bottom',
      file: jwplayer_file,
      image: jwplayer_image,
    });
  }
</script>
