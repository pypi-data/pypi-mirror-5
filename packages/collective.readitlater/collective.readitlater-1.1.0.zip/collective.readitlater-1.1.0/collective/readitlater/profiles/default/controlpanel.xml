<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="collective.readitlater"
    purge="False">

    <configlet
        title="Read it later"
        action_id="collective.readitlater"
        appId="collective.readitlater"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@collective_readitlater_settings"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
