#: ztfy/ldap/auth/__init__.py:48
msgid "Use group attribute containing it's members list"
msgstr ""
"Mode 'groupe' : utiliser un attribut du groupe contenant la liste de ses "
"membres"

#: ztfy/ldap/auth/__init__.py:49
msgid "Use principal attribute containing it's groups list"
msgstr ""
"Mode 'utilisateur' : utiliser un attribut de l'utilisateur contenant la "
"liste de ses groupes"

#: ztfy/ldap/auth/__init__.py:64
msgid "None (only use members own mail address)"
msgstr "Aucun (utiliser seulement l'adresse personnelle des membres)"

#: ztfy/ldap/auth/__init__.py:65
msgid "Use group internal attribute"
msgstr "Mode 'interne': utiliser un attribut interne du groupe"

#: ztfy/ldap/auth/__init__.py:66
msgid "Use another group internal attribute"
msgstr "Mode 'redirection': utiliser un attribut interne d'un autre groupe"

#: ztfy/ldap/auth/interfaces/__init__.py:104
msgid "Group principals attribute"
msgstr "Attribut contenant la liste des membres"

#: ztfy/ldap/auth/interfaces/__init__.py:105
msgid "Name of LDAP's group attribute containing it's principals"
msgstr ""
"En mode 'groupe', nom de l'attribut LDAP de chaque groupe contenant la liste "
"de ses membres"

#: ztfy/ldap/auth/interfaces/__init__.py:110
msgid "'group' mode query filter"
msgstr "Filtre de recherche en mode 'groupe'"

#: ztfy/ldap/auth/interfaces/__init__.py:111
msgid "LDAP query used to find principals and groups in 'group' mode"
msgstr ""
"Requête LDAP utilisée pour rechercher les membres et les groupes en mode "
"'groupe'"

#: ztfy/ldap/auth/interfaces/__init__.py:115
msgid "Principal groups attribute"
msgstr "Attribut contenant la liste des groupes"

#: ztfy/ldap/auth/interfaces/__init__.py:116
msgid "Name of LDAP's principal attribute containing it's groups"
msgstr ""
"En mode 'utilisateur', nom de l'attribut LDAP de chaque utilisateur "
"contenant la liste de ses groupes"

#: ztfy/ldap/auth/interfaces/__init__.py:121
msgid "'principal' mode query filter"
msgstr "Filtre de recherche en mode 'utilisateur'"

#: ztfy/ldap/auth/interfaces/__init__.py:122
msgid "LDAP query used to find principals and groups in 'principal' mode"
msgstr ""
"Requête LDAP utilisée pour rechercher les membres et les groupes en mode "
"'utilisateur'"

#: ztfy/ldap/auth/interfaces/__init__.py:126
msgid "Cache proxy name"
msgstr "Nom du proxy de cache"

#: ztfy/ldap/auth/interfaces/__init__.py:127
msgid "Use given cache to store principals groups"
msgstr ""
"Le cache sélectionné pourra être utilisé pour mettre la liste des groupes de "
"chaque utilisateur en cache"

#: ztfy/ldap/auth/interfaces/__init__.py:141
msgid "Group mail mode"
msgstr "Mode de messagerie des groupes"

#: ztfy/ldap/auth/interfaces/__init__.py:142
msgid "Mail mode defines how a mail can be send to group members"
msgstr ""
"Le mode de messagerie détermine la façon dont les messages seront adressés "
"aux membres d'un groupe"

#: ztfy/ldap/auth/interfaces/__init__.py:147
msgid "DN replace expression"
msgstr "Expression de remplacement du DN"

#: ztfy/ldap/auth/interfaces/__init__.py:148
msgid ""
"In 'redirect' mode, enter DN source and target value parts, separated by a "
"'|'"
msgstr ""
"En mode 'redirection', indiquez les éléments du DN source et du DN cible, "
"séparés par un '|'"

#: ztfy/ldap/auth/interfaces/__init__.py:152
msgid "Group mail attribute"
msgstr "Attribut contenant l'adresse de messagerie"

#: ztfy/ldap/auth/interfaces/__init__.py:153
msgid "Name of LDAP's group attribute containing mail address"
msgstr "Nom de l'attribut LDAP contenant l'adresse de messagerie d'un groupe"

#: ztfy/ldap/auth/interfaces/__init__.py:42
msgid "LDAP groups folder authentication plug-in"
msgstr "Plug-in d'authentification des groupes LDAP"

#: ztfy/ldap/auth/interfaces/__init__.py:65
msgid "LDAP authenticator name"
msgstr "Outil d'authentification LDAP"

#: ztfy/ldap/auth/interfaces/__init__.py:66
msgid "Name of the LDAP authenticator plug-in used to authenticate users"
msgstr "Nom de l'outil d'authentification LDAP \"natif\""

#: ztfy/ldap/auth/interfaces/__init__.py:70
msgid "Group ID prefix"
msgstr "Préfixe des IDs de groupes"

#: ztfy/ldap/auth/interfaces/__init__.py:71
msgid "Prefix to add to all groups ids"
msgstr "Préfixe à ajouter à tous les identifiants de groupes"

#: ztfy/ldap/auth/interfaces/__init__.py:74
msgid "Group title attribute"
msgstr "Attribut 'libellé'"

#: ztfy/ldap/auth/interfaces/__init__.py:75
msgid "LDAP attribute used to identify group's title"
msgstr "Nom de l'attribut LDAP utilisé pour afficher le libellé du groupe"

#: ztfy/ldap/auth/interfaces/__init__.py:80
msgid "Group description attribute"
msgstr "Attribut 'description'"

#: ztfy/ldap/auth/interfaces/__init__.py:81
msgid "LDAP attribute used to identify group's description"
msgstr "Nom de l'attribut LDAP utilisé pour afficher la description du groupe"

#: ztfy/ldap/auth/interfaces/__init__.py:86
msgid "Group required class"
msgstr "Classe requise pour un groupe"

#: ztfy/ldap/auth/interfaces/__init__.py:87
msgid "LDAP class required on groups"
msgstr "Nom de la classe LDAP obligatoire permettant d'identifier un groupe"

#: ztfy/ldap/auth/interfaces/__init__.py:92
msgid "Principal required class"
msgstr "Classe requise pour un utilisateur"

#: ztfy/ldap/auth/interfaces/__init__.py:93
msgid "LDAP class required on pincipals"
msgstr ""
"Nom de la classe LDAP obligatoire permettant d'identifier un utilisateur"

#: ztfy/ldap/auth/interfaces/__init__.py:98
msgid "Group members query mode"
msgstr "Mode de recherche des membres d'un groupe"

#: ztfy/ldap/auth/interfaces/__init__.py:99
msgid "Query mode used to find members of a group and groups of a principal"
msgstr ""
"Le mode de recherche utilisé pour rechercher les membres d'un groupe et les "
"groupes d'un membre"
